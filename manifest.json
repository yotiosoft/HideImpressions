{
    "name": "HideImpressions",
    "description": "HideImpressions",
    "version": "1.5",
    "manifest_version": 3,
    "permissions": ["tabs"],
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*", "https://x.com/*"],
            "js": ["hide_impressions.js"],
            "all_frames": true,
            "run_at": "document_end"
        }
    ]
}
